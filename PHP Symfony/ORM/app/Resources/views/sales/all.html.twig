<table border="1">
    <tr>
        <th>Customer Name</th>
        <th>Car Make</th>
        <th>Car Model</th>
        <th>Options</th>
    </tr>
    {% for sale in sales %}
        <tr>
            <td>{{ sale.name }}</td>
            <td>{{ sale.make }}</td>
            <td>{{ sale.model }}</td>
        </tr>
    {% endfor %}
</table>